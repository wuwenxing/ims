                                <option value="1" selected>启用</option>
                                <option value="0">禁用</option>
                            </select>
                        </div>
                    </td>
                </tr>
                <tr>
                    <td>
                        <div class="layui-inline">
                            <label for="" class="layui-form-label">物品有效时间</label>
                        </div>
                    </td>
                    <td colspan="3">
                        <div class="layui-inline">
                            <input type="text" class="layui-input" id="date1" placeholder="yyyy-mm-dd hh:mm:ss" name="startTime" lay-verType="tips">
                        </div>
                        <div class="layui-inline"> 至 </div>
                        <div class="layui-inline">
                            <input type="text" class="layui-input" id="date2" placeholder="yyyy-mm-dd hh:mm:ss" name="endTime" lay-verType="tips">
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
        <div style="display:none;">
            <button class="layui-btn addPrize" lay-submit lay-filter="addPrize"></button>
        </div>
    </form>
<%}%>