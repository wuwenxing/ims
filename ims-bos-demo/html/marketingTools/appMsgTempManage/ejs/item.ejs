<% if(typeof(id) != "undefined"){ %>
  <form class="layui-form" action="">
      <div class="layui-form-item">
          <div class="layui-inline">
            <label class="layui-form-label">编号</label>
            <div class="layui-input-inline">
              <input name="code" value="<%=code%>" lay-verify="required" lay-verType="tips" autocomplete="off" class="layui-input">
            </div>
          </div>
          <div class="layui-inline">
            <label class="layui-form-label">状态</label>
            <div class="layui-input-inline">
              <select name="enableFlag" id="" lay-verify="required"  lay-verType="tips">
                <option value="Y" <%if(enableFlag == 'Y'){%>selected<%}%> >启用</option>
                <option value="N" <%if(enableFlag == 'N'){%>selected<%}%>>关闭</option>
              </select>
            </div>
          </div>
      </div>
      <div class="layui-form-item">
          <div class="layui-inline">
            <label class="layui-form-label">语言</label>
            <div class="layui-input-inline">
                <div class="layui-input-inline">
                  <select name="lang" id="">
                    <option value="">---请选择---</option>
                    <option value="zh_TW" <%if(lang == 'zh_TW'){%>selected<%}%>>繁体</option>
                    <option value="zh_CN" <%if(lang == 'zh_CN'){%>selected<%}%>>简体</option>
                    <option value="en_US" <%if(lang == 'en_US'){%>selected<%}%>>英文</option>
                  </select>
                </div>
            </div>
          </div>
          <div class="layui-inline">
            <label class="layui-form-label">名称</label>
            <div class="layui-input-inline">
                <input type="tel" name="tplName" value="<%=tplName%>" lay-verify="required"  lay-verType="tips" autocomplete="off" class="layui-input">
            </div>
          </div>
      </div>
      <div class="layui-form-item">
          <div class="layui-inline">
            <label class="layui-form-label">推送的APP</label>
              <input 
                class="layui-input-inline" 
                type="checkbox" 
                name="pushApp" 
                value="GWFX_GTS2_APP" 
                lay-skin="primary" 
                title="FX GTS2 APP" 
                <%if(pushApp == 'GWFX_GTS2_APP'){%>checked<%}%> >
              <input 
                class="layui-input-inline" 
                type="checkbox" 
                name="pushApp" 
                value="HX_GTS2_APP"  
                lay-skin="primary" 
                title="HX GTS2 APP" 
                <%if(pushApp == 'HX_GTS2_APP'){%>checked<%}%>>
              <input 
                class="layui-input-inline" 
                type="checkbox" 
                name="pushApp" 
                value="FXDEMO_GTS2_APP"  
                lay-skin="primary" 
                title="FXDEMO GTS2 APP" 
                <%if(pushApp == 'FXDEMO_GTS2_APP'){%>checked<%}%>>
              <input 
                class="layui-input-inline" 
                type="checkbox" 
                name="pushApp" 
                value="PM_GTS_APP"  
                lay-skin="primary" 
                title="PM GTS APP" 
                <%if(pushApp == 'PM_GTS_APP'){%>checked<%}%>>
          </div>
      </div>
      <div class="layui-form-item">
        <div class="layui-inline">
          <label for="" class="layui-form-label">推送对象</label>
          <div class="layui-input-inline">
            <select name="pushType" id="">
              <option value="">---请选择---</option>
              <option value="all" <%if(pushType == 'all'){%>selected<%}%>>推送所有人指定设备(all)</option>
              <option value="tag" <%if(pushType == 'tag'){%>selected<%}%>>指定分组的人(tag)</option>
              <option value="alias" <%if(pushType == 'alias'){%>selected<%}%>>指定人(alias)</option>
              <option value="device" <%if(pushType == 'device'){%>selected<%}%>>指定设备(device)</option>
            </select>
          </div>
        </div>
      </div>
      <div class="layui-form-item">
          <div class="layui-inline">
            <label class="layui-form-label">展示位置</label>
            <div class="layui-input-inline">
              <input 
                class="layui-input-inline" 
                type="checkbox" 
                name="msgShowPosition" 
                value="1" 
                lay-skin="primary" 
                <%if(msgShowPosition.indexOf('1') != '-1'){%>checked<%}%>
                title="弹出窗">
              <input 
                class="layui-input-inline"  
                type="checkbox" 
                name="msgShowPosition" 
                value="2"  
                <%if(msgShowPosition.indexOf('2') != '-1'){%>checked<%}%>
                lay-skin="primary" 
                title="跑马灯">
              <input 
                class="layui-input-inline"  
                type="checkbox" 
                name="msgShowPosition" 
                value="3"  
                <%if(msgShowPosition.indexOf('3') != '-1'){%>checked<%}%>
                lay-skin="primary" 
                title="工具栏">
            </div>
          </div>
          <div class="layui-inline">
            <label class="layui-form-label">内容类型</label>
            <div class="layui-input-inline">
                <select name="tplContentType" id="" lay-verify="required" lay-verType="tips">
                  <option value=""  <%if(tplContentTypeTxt == null){%>selected<%}%>>---请选择---</option>
                  <option value="1" <%if(tplContentTypeTxt == '1'){%>selected<%}%> >图片</option>
                  <option value="2" <%if(tplContentTypeTxt == '2'){%>selected<%}%>>图文</option>
                  <option value="3" <%if(tplContentTypeTxt == '3'){%>selected<%}%>>其他</option>
                </select>
            </div>
          </div>
      </div>
  
      <div class="layui-form-item">
        <div class="layui-inline">
          <label for="" class="layui-form-label">内容</label>
          <div class="layui-input-block">
            <textarea name="tplContent" id="" lay-verify="required" lay-verType="tips" cols="30" rows="10"><%=tplContent%></textarea>
          </div>
        </div>
      </div>
      <div class="layui-form-item">
        <div class="layui-inline">
          <label for="" class="layui-form-label">摘要</label>
          <div class="layui-input-block">
            <textarea name="summary" id="" cols="30" rows="10"><%=summary%></textarea>
          </div>
        </div>
      </div>
      <div class="layui-form-item">
        <div class="layui-inline">
          <label for="" class="layui-form-label">备注</label>
          <div class="layui-input-block">
            <textarea name="remark" id="" cols="30" rows="10"><%=remark%></textarea>
          </div>
        </div>
      </div>
      <div class="layui-form-item">
        <div class="layui-inline">
          <label for="" class="layui-form-label">其他信息</label>
          <div class="layui-input-block">
            <textarea name="otherMsg" id="" cols="30" rows="10">
              
            </textarea>
            <p style="color:red;">(放置地址等信息，请输入json格式)</p>
          </div>
        </div>
      </div>
      <button class="layui-btn node-submit" style="display:none;" lay-submit lay-filter="item"></button>
    </form>
  <% }else{ %>
