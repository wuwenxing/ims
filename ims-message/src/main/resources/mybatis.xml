		<typeAlias alias="Gts2Deal" type="com.gwghk.ims.message.dao.entity.Gts2Deal" />
		<typeAlias alias="Gts2Customer" type="com.gwghk.ims.message.dao.entity.Gts2Customer" />
		<typeAlias alias="Gts2AccountInfo" type="com.gwghk.ims.message.dao.entity.Gts2AccountInfo" />
		<typeAlias alias="Gts2CashinProposal" type="com.gwghk.ims.message.dao.entity.Gts2CashinProposal" />
