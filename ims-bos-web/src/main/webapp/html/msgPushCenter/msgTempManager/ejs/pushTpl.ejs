<%if(pushType == "APP"){%>
<div class="layui-form-item pad-30 flex">
  <div class="layui-inline flex flex-item no-mr">
    <label for="" class="layui-form-label flex flex-align-items">
      <em class="required">*</em>
      <span>推送的APP</span>
    </label>
    <div class="layui-inline flex-item">
      <input class="layui-inline" type="checkbox" name="pushAppList" 
        value="FX_GTS2" lay-skin="primary" title="FX_GTS2" lay-filter="pushAppSel">
      <input class="layui-inline" type="checkbox" name="pushAppList" 
        value="FXDEMO_GTS2" lay-skin="primary" title="FXDEMO_GTS2" lay-filter="pushAppSel">
    </div>
  </div>
</div>
<%}else if(pushType == "PCUI"){%>
<div class="layui-form-item pad-30 flex">
  <div class="layui-inline flex flex-item no-mr">
    <label for="" class="layui-form-label flex flex-align-items">
      <em class="required">*</em>
      <span>推送的PCUI</span>
    </label>
    <div class="layui-inline flex-item">
      <input class="layui-inline" type="checkbox" name="pushPcuiTypeList" 
        value="FX GTS2 PCUI" lay-skin="primary" title="FX GTS2 PCUI" lay-filter="pushPcuiSel">
    </div>
  </div>
</div>
<%}%>